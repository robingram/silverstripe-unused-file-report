    "authors": [
        {
            "name": "Rob Ingram",
            "email": "robert.ingram@ccc.govt.nz"
        }
    ],
    "require": {
        "silverstripe/assets": "^2"
    },
    "autoload": {
        "psr-4": {
            "RobIngram\\SilverStripe\\UnusedFileReport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
